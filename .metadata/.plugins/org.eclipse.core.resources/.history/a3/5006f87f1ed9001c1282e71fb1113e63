package ch14;

public class TakeTransTest {

	public static void main(String[] args) {

		Student studentKim = new Student("JB", 10000);
		Student studentLee = new Student("DB", 5000);
		
		Bus bus = new Bus(5633);
		Subway subway = new Subway(1);
		
		studentKim.takeBus(bus);
		studentKim.takeSubway(subway);
		studentLee.takeBus(bus);
		studentLee.takeSubway(subway);
		
		
	}

}
